windows.debug.x86_32 =   "res://addons/godot-rapier-3d/bin/i686-pc-windows-msvc-godot_rapier_3d.dll"
windows.release.x86_32 = "res://addons/godot-rapier-3d/bin/i686-pc-windows-msvc-godot_rapier_3d.dll"
windows.debug.x86_64 =   "res://addons/godot-rapier-3d/bin/x86_64-pc-windows-msvc-godot_rapier_3d.dll"
