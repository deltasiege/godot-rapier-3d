[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
reloadable = true

[icons]
RapierRigidBody3D = "res://addons/godot-rapier-3d/icons/rb3d.svg"
RapierCollider3D = "res://addons/godot-rapier-3d/icons/col3d.svg"

[libraries]
linux.debug.x86_64 =     "res://addons/godot-rapier-3d/rust/target/debug/libgodot_rapier_3d.so"
linux.release.x86_64 =   "res://addons/godot-rapier-3d/rust/target/release/libgodot_rapier_3d.so"
windows.debug.x86_64 =   "res://addons/godot-rapier-3d/rust/target/debug/godot_rapier_3d.dll"
windows.release.x86_64 = "res://addons/godot-rapier-3d/rust/target/release/godot_rapier_3d.dll"
macos.debug =            "res://addons/godot-rapier-3d/rust/target/debug/libgodot_rapier_3d.dylib"
macos.release =          "res://addons/godot-rapier-3d/rust/target/release/libgodot_rapier_3d.dylib"
macos.debug.arm64 =      "res://addons/godot-rapier-3d/rust/target/debug/libgodot_rapier_3d.dylib"
macos.release.arm64 =    "res://addons/godot-rapier-3d/rust/target/release/libgodot_rapier_3d.dylib"